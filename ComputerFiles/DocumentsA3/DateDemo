import java.util.*;
import java.text.*;
import java.util.Scanner;

public class Date {
  public static void main(String args[]) {
      Date dNow = new Date();
      SimpleDateFormat ft = 
      new SimpleDateFormat ("dd/MM/yyyy");

      System.out.println("Current Date: " + ft.format(dNow));
      
      Scanner keyboard = new Scanner(System.in); 
      
    }
}
